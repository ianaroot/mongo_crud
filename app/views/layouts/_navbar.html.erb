<% if current_user %>
  <%= link_to "Sign out", session_path, method: :delete %>
<% else %>
  <%= form_tag sessions_path do %>

    <%= label_tag :email %>
    <%= text_field_tag :email %>

    <%= label_tag :password %>
    <%= password_field_tag :password %>

    <%= submit_tag "Sign in" %>
  <% end %>
<% end %>